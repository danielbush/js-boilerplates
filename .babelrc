{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        // Minimum verisions to support:
        "targets": {
          "browsers": "> 5%"
          // "chrome": "50",
          // "ie": "11"
        },
        // TODO: Maybe set to false if you're a lib:
        "useBuiltIns": "entry",
        "corejs": 3
      }
    ],
    // babel-preset-css-prop will generate code with 'import' statements.
    // It must be run BEFORE @babel/preset-env.
    "@emotion/babel-preset-css-prop",
  ],
  "plugins": [
    // Proposals probably aren't in babel-env, so we the ones we want here:
    "@babel/proposal-class-properties",
    "@babel/plugin-proposal-export-default-from"
  ],
  "env": {
    "test": {
      "plugins": []
    }
  }
}