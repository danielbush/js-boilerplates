{
  "name": "babel-boilerplate",
  "version": "1.0.0",
  "description": "babel boilerplate",
  "main": "lib/index.js",
  "repository": "tbc",
  "author": "Daniel Bush",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/ test/",
    "check": "npm run lint",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "rm -rf lib; npm run build:types && npm run build:js",
    "build:js": "babel --extensions '.ts,.tsx,.js,.jsx' -s --out-dir lib src",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "babel-node --extensions '.ts,.tsx,.js,.jsx' src/index.js",
    "start": "node lib/index.js",
    "test": "npm run test:unit",
    "test:unit:cmd": "NODE_ENV=test mocha -r test/setup/unit src/**/*.spec.ts{,x} src/**/*.spec.js{,x} test/unit",
    "test:unit": "nyc --all -- npm run test:unit:cmd",
    "test:watch": "npm run test:unit:cmd -- --watch",
    "prepublish": "in-publish && { echo 'Prepublish:' && npm run build; } || not-in-publish",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "config": "^3.3.1",
    "core-js": "^3.6.5",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "changelog-verify": "^1.0.4",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "in-publish": "^2.0.1",
    "mocha": "^7.0.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.0",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.5.0",
    "typescript": "^3.8.3",
    "version-changelog": "^3.0.0"
  },
  "peerDependencies": {}
}
