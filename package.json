{
  "name": "babel-boilerplate",
  "version": "1.0.0",
  "description": "babel boilerplate",
  "main": "lib/index.js",
  "repository": "tbc",
  "author": "Daniel Bush",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/ test/",
    "check:types": "tsc --noEmit",
    "check:types:watch": "npm run check:types -- --watch",
    "check": "npm run lint && npm run check:types",
    "build": "rm -rf lib; npm run build:types && npm run build:js",
    "build:js": "babel --extensions '.ts,.tsx,.js,.jsx' -s --out-dir lib src",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "babel-node --extensions '.ts,.tsx,.js,.jsx' src/index.js",
    "start": "node lib/index.js",
    "test": "npm run test:unit",
    "test:unit:cmd": "NODE_ENV=test mocha -r test/setup/unit src/*.spec.[jt]s{,x} src/**/*.spec.[jt]s{,x} test/unit",
    "test:unit": "nyc --all -- npm run test:unit:cmd",
    "test:watch": "npm run test:unit:cmd -- --watch",
    "prepublish": "in-publish && { echo 'Prepublish:' && npm run build; } || not-in-publish",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "config": "^3.3.6",
    "core-js": "^3.10.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "changelog-verify": "^1.0.4",
    "dirty-chai": "^2.0.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "in-publish": "^2.0.1",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.6.0",
    "typescript": "^4.2.3",
    "version-changelog": "^3.0.0"
  },
  "peerDependencies": {}
}
