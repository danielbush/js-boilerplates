{
  "name": "babel-react-boilerplate",
  "version": "1.0.0",
  "description": "babel-react boilerplate",
  "main": "lib/index.js",
  "repository": "tbc",
  "author": "Daniel Bush",
  "license": "MIT",
  "scripts": {
    "boilerplate": "webpack serve --config webpack.boilerplate.js",
    "lint": "eslint --no-error-on-unmatched-pattern src/ test/",
    "check:types": "tsc --noEmit",
    "check:types:watch": "npm run check:types -- --watch",
    "check": "npm run lint && npm run check:types",
    "build": "rm -rf lib; npm run build:types && npm run build:js",
    "build:js": "babel --extensions '.ts,.tsx,.js,.jsx' -s --out-dir lib src",
    "build:types": "tsc --emitDeclarationOnly",
    "build:dist": "webpack --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "test": "npm run test:unit",
    "test:unit:cmd": "NODE_ENV=test jest",
    "test:unit": "npm run test:unit:cmd -- --coverage",
    "test:watch": "npm run test:unit:cmd -- --watch",
    "jest:help": "jest --help",
    "prepublish": "in-publish && { echo 'Prepublish:' && npm run build; } || not-in-publish",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "classnames": "^2.3.1",
    "config": "^3.3.6",
    "core-js": "^3.10.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/parser": "^4.21.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "changelog-verify": "^1.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "in-publish": "^2.0.1",
    "jest": "^26.6.3",
    "jsdom": "^16.5.2",
    "postcss": "^8.2.9",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^7.0.2",
    "postcss-scss": "^3.0.5",
    "postcss-url": "^10.1.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3",
    "typescript-plugin-css-modules": "^3.2.0",
    "uglify-js": "^3.13.3",
    "version-changelog": "^3.1.1",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "peerDependencies": {}
}
