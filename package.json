{
  "name": "babel-boilerplate",
  "version": "1.0.0",
  "description": "babel boilerplate",
  "main": "lib/index.js",
  "repository": "tbc",
  "author": "Daniel Bush",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/ test/",
    "check": "yarn run lint",
    "check:env": "node -e \"require('dotenv-safe').load()\"",
    "bundle": "rm build/*; webpack && uglifyjs -o build/dist.js build/main.js && cat build/dist.js | gzip - >build/dist.js.gz",
    "build": "rm -rf lib; babel -s --out-dir lib src",
    "start": "babel-node src/index.js",
    "test": "npm run test:unit",
    "test:unit:cmd": "NODE_ENV=test nyc --all -- mocha --opts mocha.opts -r test/setup/unit src/**/*.spec.js test/unit",
    "test:unit": "nyc --all -- npm run test:unit:cmd",
    "test:watch": "npm run test:unit:cmd -- --watch",
    "webpack": "webpack --config webpack.config.js",
    "prepublish": "in-publish && { echo 'Prepublish:' && yarn run build; } || not-in-publish",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "config": "^3.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "changelog-verify": "^1.0.4",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "in-publish": "^2.0.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.0",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.5.0",
    "version-changelog": "^3.0.0"
  },
  "peerDependencies": {}
}
