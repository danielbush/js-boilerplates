{
  "name": "babel-boilerplate",
  "version": "1.0.0",
  "description": "babel boilerplate",
  "main": "lib/index.js",
  "repository": "tbc",
  "author": "Daniel Bush",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/ test/",
    "check": "yarn run lint",
    "check:env": "node -e \"require('dotenv-safe').load()\"",
    "bundle": "rm build/*; webpack && uglifyjs -o build/dist.js build/main.js && cat build/dist.js | gzip - >build/dist.js.gz",
    "build": "rm -rf lib; babel -s --out-dir lib src",
    "start": "babel-node src/index.js",
    "test": "npm run test:unit",
    "test:unit": "mocha -r babel-polyfill -r @babel/register -r test/setup/unit src test/unit",
    "test:watch": "npm run test:unit -- --watch",
    "webpack": "webpack --config webpack.config.js",
    "prepublish": "in-publish && { echo 'Prepublish:' && yarn run build; } || not-in-publish",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "dotenv-safe": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "changelog-verify": "^1.0.4",
    "dirty-chai": "^2.0.1",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "in-publish": "^2.0.0",
    "mocha": "^5.0.0",
    "prettier": "^1.15.3",
    "sinon": "^7.0.0",
    "sinon-chai": "^3.0.0",
    "version-changelog": "^3.0.0"
  },
  "peerDependencies": {}
}
